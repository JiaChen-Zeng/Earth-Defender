const _math_var -> _lib_var + 400  # 200 vars

def mod(a, b)
	out -> a - ((a / b) * b)

def max(a, b)
	out -> cond(a < b, b, a)

def min(a, b)
	out -> cond(a < b, a, b)

def abs(number)
	out -> cond(number > 0, number, -number)

const _randomSeed -> _math_var
memory[_randomSeed] -> 987654321
const RANDOM_MULTIPLIER -> 543
const RANDOM_OFFSET -> 87654
def random(number)
	memory[_randomSeed] -> RANDOM_MULTIPLIER * memory[_randomSeed] + RANDOM_OFFSET
	out -> mod(memory[_randomSeed], number)